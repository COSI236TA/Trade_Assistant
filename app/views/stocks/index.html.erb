<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h1>Stocks List</h1>
    <table class="table">
      <thead>
        <tr>
          <th>Ticker</th>
          <th>Price</th>
          <th>Last Updated Time</th>
        </tr>
      </thead>
      <tbody>
        <% @stocks.each do |stock| %>
          <tr>
            <td><%= stock.ticker %></td>
            <td><%= stock.last_trade_price_only %></td>
            <td><%= stock.updated_at %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

