<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h1>Portfolio List</h1>
    <table class="table">
      <thead>
        <tr>
          <th>Portfolio Name</th>
          <th>Portfolio Description</th>
        </tr>
      </thead>
      <tbody>
        <% @portfolios.each do |portfolio| %>
        <tr>
          <td><%= portfolio.name %></td>
          <td><%= portfolio.description %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
